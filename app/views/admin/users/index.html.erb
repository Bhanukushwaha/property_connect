<h2>wolcome user</h2>
<table>
	<tr>
		<th>Email</th>
		<th>company_name</th>
		<th>Role</th>
	</tr>
	<tbody>
		<% @users.each do |user| %>
			<tr>
				<td><%= user.email %></td>
				<td><%= user&.company&.title %></td>
				<td><%= user.role %></td>
				<td><%= link_to "show", admin_user_path(user), class: "btn btn-outline-primary" %></td>
			  <td><%= link_to "Edit", edit_admin_user_path(user), class: "btn btn-outline-success" %></td>
			  <td><%= button_to 'Destroy', admin_user_path(user), method: :delete, data: { confirm: 'Are you sure?' }, class: "w3-bar-item w3-button" %></td>
			</tr>
		<% end %>
	</tbody>
</table>
<%= link_to "new", new_admin_user_path %>
