<script type="">    
  var currentLatitude; // Declare variables to store latitude and longitude
  var currentLongitude;

  if ("geolocation" in navigator) {
    navigator.geolocation.getCurrentPosition(function (position) {
      currentLatitude = position.coords.latitude; // Store latitude and longitude
      currentLongitude = position.coords.longitude;

      // Update the HTML to display the current location
      document.getElementById('latitude').textContent = currentLatitude;
      document.getElementById('longitude').textContent = currentLongitude;
    }, function (error) {
      console.error("Error getting location:", error.message);
    });
  } else {
    alert("Geolocation is not supported by your browser.");
  }

  document.getElementById('location').addEventListener('change', function () {
  var selectedValue = this.value; // Get the selected value from the dropdown
  if (selectedValue) {
    // Combine the selected value with current latitude and longitude
    var locationQuery = "latitude=" + currentLatitude + "&longitude=" + currentLongitude + "&location=" + selectedValue;

    // Redirect to the search page with the combined location query
    window.location.href = "/search?" + locationQuery;
  }
});

</script>