<%#= link_to "Edit this profile", edit_profile_path(@profile) %>
    
<div class="page-banner-section section">
  <div class="container">
    <div class="row">
      <div class="col">
        <h1 class="page-banner-title">Profile Details</h1>        
      </div>
    </div>
  </div>
</div>
<!--Page Banner Section end-->
<!--Agent Section start-->
<div class="agent-section section pt-100 pt-lg-80 pt-md-70 pt-sm-60 pt-xs-50 pb-70 pb-lg-50 pb-md-40 pb-sm-30 pb-xs-20">
  <div class="container">
    <div class="row row-25">
      <!--Agent Image-->
      <div class="col-lg-5 col-12 mb-30">
        <div class="agent-image">
          <% if @profile.image.attached? %>
            <%= image_tag @profile.image, style: "width: 470px; height: 470px; " %>
          <% else %>
          <img src="/assets/web/agent-1.jpg" alt="">
          <% end %>
        </div>
      </div>
      <!--Agent Content-->
      <div class="col-lg-7 col-12">
        <div class="agent-content">
          <h3 class="title"><%= @profile.name %></h3>
          <p><%= @profile.about %></p>
          <div class="row">
            <div class="col-md-6 col-12 mb-30">
              <h4>Persoanl Info</h4>
              <ul>
                <li><i class="pe-7s-map-marker"></i><%= @profile.address %></li>
                <li><i class="pe-7s-phone"></i><%= @profile.mobile_number %></li>
                <% if current_user.role == "agent" %>   
                  <li><i class="pe-7s-photo"></i><%= current_user.properties.count %> Properties</li>
                <% end %>
              </ul>
              <!-- <div class="social">
